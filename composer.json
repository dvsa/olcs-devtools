{
    "name": "olcs/olcs-devtools",
    "description": "Dev tools",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/OLCS",
            "type": "composer"
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/Packagist-Proxy",
            "type": "composer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "brianium/paratest": "^3.0",
        "dms/phpunit-arraysubset-asserts": "^0.1",
        "olcs/coding-standards": "~4.206.0",
        "squizlabs/php_codesniffer": "~3.1",
        "phpunit/phpunit": "8.*",
        "phpunit/phpcov": "6.*",
        "mockery/mockery": "1.2.4",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpmd/phpmd": "^2.8.2",
        "johnkary/phpunit-speedtrap": "^3.2",
        "mikey179/vfsstream": "~1.4.0",
        "elendev/nexus-composer-push": "^0.1"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "Devtools\\": "src/"
        }
    },
    "bin": [
        "bin/review",
        "bin/composer-install-syms",
        "bin/composer-update-syms",
        "bin/release"
    ],
    "extra": {
        "nexus-push": {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/olcs-devtools/"
        }
    }
}
